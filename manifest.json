{
    "name": "LinkedIn Curious",
    "version": "1.0.1",
    "description": "Add back the curious reaction to LinkedIn",
    "manifest_version": 3,
    "author": "Mordechai Meisels",
    "content_scripts": [
        {
            "js": [
                "scripts/script.js"
            ],
            "matches": [
                "https://linkedin.com/*",
                "https://*.linkedin.com/*"
            ]
        }
    ],
    "icons": {
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}